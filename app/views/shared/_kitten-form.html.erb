<%= form_for @kitten do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.select :age, options_for_select([[1,1],[2,2],[3,3],[4,4]]) %>

  <%= f.label :cuteness %>
  <%= f.text_field :cuteness %>

  <%= f.label :softness %>
  <%= f.text_field :softness %>
  <br>
  <%= f.submit yield(:btn_title) %>
<% end %>